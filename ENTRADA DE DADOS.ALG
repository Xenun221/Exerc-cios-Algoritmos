Algoritmo "Entrada de dados"
Var

   nome: caractere
   idade: inteiro
   prof: caractere

inicio
escreva("Digite seu nome: ")
leia(nome)
escreva("Digite sua idade: ")
leia(idade)
escreva("Qual e sua profissão: ")
leia(prof)

escreval("Seu nome é ", nome, " tem ", idade, " anos de idade ")
escreval("E sua profissão é ", prof)

Fimalgoritmo